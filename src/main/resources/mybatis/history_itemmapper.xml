<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kkurumi.mapper.History_itemMapper">


    <insert id="insert" parameterType="History_item">
        INSERT INTO history_item VALUES (#{item_id},#{user_pin}, TO_CHAR(sysdate, 'YYYY-MM-DD HH24MISS'))
    </insert>


</mapper>